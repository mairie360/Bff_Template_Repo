{
  "name": "bff-template",
  "version": "1.0.0",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "openapi:generate": "openapi-typescript-codegen --input ./openapi.yaml --output ./src/services/generated --client axios",
    "test": "jest"
  },
  "dependencies": {
    "axios": "1.12.2",
    "dotenv": "17.2.3",
    "express": "5.1.0"
  },
  "devDependencies": {
    "@types/express": "5.0.3",
    "@types/jest": "30.0.0",
    "@types/supertest": "6.0.3",
    "@typescript-eslint/eslint-plugin": "8.45.0",
    "@typescript-eslint/parser": "8.45.0",
    "eslint": "9.37.0",
    "jest": "30.2.0",
    "openapi-typescript-codegen": "0.29.0",
    "supertest": "7.1.4",
    "ts-jest": "29.4.4",
    "ts-node": "10.9.2",
    "typescript": "5.9.3"
  }
}
